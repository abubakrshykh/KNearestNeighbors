      "source": [
        "#ASSIGNING DEPENDENT AND INDEPENDENT VARIABLES\n",
        "\n",
        "X = dataset.iloc[:,:-1].values\n",
        "y = dataset.iloc[::,-1].values\n",
        "\n"
      ],
      "metadata": {
        "id": "Vq_nBJKtUR9y"
      },
      "execution_count": 16,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#LOADING THE DATA\n",
        "#PLAYING WITH HYPERPARAMETRS THROUGH GRIDSEARCHCV TO GET BEST FIT\n",
        "from sklearn.model_selection import GridSearchCV\n",
        "from sklearn.neighbors import KNeighborsClassifier\n",
        "\n",
        "model = KNeighborsClassifier()\n",
        "parameters = {\n",
        "    'n_neighbors': [1,2,3],\n",
        "    'metric': ['euclidean', 'minkowski','Manhattan']\n",
        "}\n",
        "\n",
        "\n",
        "grid_search = GridSearchCV(estimator=model, param_grid= parameters, cv=5)\n",
        "grid_search.fit(X,y)\n"
      ],
      "metadata": {
        "id": "rUIJiMEKUeBA"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#SHOWING THE BEST PARAMETERS\n",
        "print(grid_search.best_params_)"
